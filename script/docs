#!/usr/bin/env node

var fetchDocs = require('./fetch-docs.js')

var args = process.argv.slice(2)

var setup = {
  latest: false,
  version: 'master',
  finalDir: '_docs'
}

if (args[0] && args[0] !== '--latest') setup.version = args[0]
if (args[1] && args[1] === '--latest') setup.latest = true


fetchDocs(setup)
